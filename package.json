{
  "name": "bunn-aws",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "tw:fix": "pnpm dlx @tailwindcss/upgrade --force",
    "docker:start": "docker info >/dev/null 2>&1 || (echo '启动 Docker Desktop...' && open -a Docker && until docker info >/dev/null 2>&1; do sleep 1; done && echo 'Docker 已就绪')",
    "docker:up": "pnpm docker:start && docker compose up -d postgres redis",
    "docker:down": "docker compose down",
    "docker:reset": "pnpm docker:start && docker compose down -v && docker compose up -d postgres redis",
    "docker:logs": "docker compose logs -f",
    "docker:app": "pnpm docker:start && docker compose --profile full up -d",
    "cdk:install": "cd cdk && pnpm install",
    "cdk:synth": "cd cdk && pnpm cdk synth",
    "cdk:diff": "cd cdk && pnpm cdk diff",
    "cdk:deploy": "cd cdk && pnpm cdk deploy --all"
  },
